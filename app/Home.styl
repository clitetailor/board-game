.Home
	height: 100%;
	width: 100%;
		
	.bg-container
		display: flex;
		flex-flow: row nowrap;
		height: 100%;
		width: 100%;
			
		.bg-paper
			flex: 1 1 0;
			height: 100%;
			background-color: white;

			box-shadow: 0 5px 20px #333;

		.bg-background
			display: flex;
			justify-content: center;

			flex: 1 1 0;
			height: 100%;
			overflow: hidden;

			z-index: -1;